#include<iostream>
#include<vector>
#include<algorithm>
#include<queue>
#include<deque>
#include<stack>
#include<set>
#include<map>
#include<random>
#include<time.h>
#include<unordered_map>
#include<bitset>
using namespace std;
using ll = long long;
const int k = 26;
struct Node {
    int suf, parent, h;
    vector<int> next;
    Node() {
        suf = -1;
        parent = -1;
        h = 0;
        next = vector<int>(k, -1);
    }
};
ll ans = 0;
vector<Node> s;
int add(int a, int ch) {
    int b = s.size();
    s.push_back({});
    s[b].suf = 0;
    s[b].parent = a;
    s[b].h = s[a].h + 1;
    for (; a != -1; a = s[a].suf) {
        if (s[a].next[ch] == -1) {
            s[a].next[ch] = b;
            continue;
        }
        int c = s[a].next[ch];
        if (s[c].parent == a) {
            s[b].suf = c;
            break;
        }
        int d = s.size();
        s.push_back(Node());
        s[d].parent = a;
        s[d].suf = s[c].suf;
        s[d].h = s[a].h + 1;
        s[c].suf = d;
        s[b].suf = d;
        for (int i = 0; i < k; ++i) {
            s[d].next[i] = s[c].next[i];
        }
        for (; a != -1 && s[a].next[ch] == c; a = s[a].suf) {
            s[a].next[ch] = d;
        }
        break;
    }
    ans += s[b].h - s[s[b].suf].h;
    return b;
}

int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(0);
    cout.tie(0);
    s.push_back({});
    int last = 0;
    string a;
    cin >> a;
    for (auto c : a) {
        last = add(last, c - 'a');
        cout << ans << '\n';
    }
}
